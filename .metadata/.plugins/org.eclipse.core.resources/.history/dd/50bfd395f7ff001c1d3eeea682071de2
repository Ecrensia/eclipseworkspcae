package clothes;

public class Top {
	String typeOfTop;
	String colorOfTop;
	String rightForPlace1;
	String rightForPlace2;
	String rightForPlace3;
	
	public Top(String typeOfTop, String colorOfTop, String rightForPlace1, String rightForPlace2,
			String rightForPlace3) {
		this.typeOfTop = typeOfTop;
		this.colorOfTop = colorOfTop;
		this.rightForPlace1 = rightForPlace1;
		this.rightForPlace2 = rightForPlace2;
		this.rightForPlace3 = rightForPlace3;
	}
}
