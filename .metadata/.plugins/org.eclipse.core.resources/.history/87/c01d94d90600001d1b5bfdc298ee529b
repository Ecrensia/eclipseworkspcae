package clothes;

import java.util.ArrayList;

public class Bag {
	String typeOfBag;
	String colorOfBag;
	String rightForPlace1;
	String rightForPlace2;
	String rightForPlace3;
	
	ArrayList<Bag> bagList = new ArrayList<>();
	
	public Bag(String typeOfBag, String colorOfBag, String rightForPlace1, String rightForPlace2,
			String rightForPlace3) {
		this.typeOfBag = typeOfBag;
		this.colorOfBag = colorOfBag;
		this.rightForPlace1 = rightForPlace1;
		this.rightForPlace2 = rightForPlace2;
		this.rightForPlace3 = rightForPlace3;
	}
}