package clothes;

import java.util.ArrayList;

public class Bottom {
	String typeOfBottom;
	String colorOfBottom;
	String rightForPlace1;
	String rightForPlace2;
	String rightForPlace3;
	
	ArrayList<Bottom> bottomList  = new ArrayList<>();
	
	public Bottom(String typeOfBottom, String colorOfBottom, String rightForPlace1, String rightForPlace2,
			String rightForPlace3) {
		this.typeOfBottom = typeOfBottom;
		this.colorOfBottom = colorOfBottom;
		this.rightForPlace1 = rightForPlace1;
		this.rightForPlace2 = rightForPlace2;
		this.rightForPlace3 = rightForPlace3;
	}
}